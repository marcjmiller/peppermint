{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "seed": "prisma db seed",
    "run:dev": "yarn migrate && yarn generate && yarn seed && yarn dev",
    "migrate-dev": "prisma migrate dev --preview-feature",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "cypress": "cypress open --config-file cypress/config/development.json",
    "server": "docker-compose --profile test up",
    "e2e": "start-server-and-test server http://localhost:5001 test:e2e",
    "e2e:open": "start-server-and-test server http://localhost:5001 cypress",
    "test:e2e": "npx cypress run --browser firefox --config-file cypress/config/development.json",
    "docker": "yarn run migrate && yarn run generate && yarn run seed && yarn start",
    "pm2-prod": "yarn run migrate && yarn run generate && yarn run seed && yarn run build && pm2 start npm --name \"next\" -- start"
  },
